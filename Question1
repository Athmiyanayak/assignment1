public class assignment1{
    public static void main(String[] args) {
        int max_sales = 0;
        int[] sales = {20, 100, 40, 60};
        
        for (int i = 0; i < sales.length; i++) { // Corrected 'lenth' to 'length'
            if (max_sales < sales[i]) {
                max_sales = sales[i];
            }
        }
        
        // Corrected the print statement
        System.out.println("Maximum sales is " + max_sales);
    }
}
